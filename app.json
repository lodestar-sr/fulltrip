{
  "expo": {
    "name": "FullTrip",
    "slug": "fulltrip",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.groweb.fulltrip",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover"
      }
    },
    "android": {
      "package": "com.groweb.fulltrip",
      "versionCode": 1,
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover"
      }
    }
    
  }
}
